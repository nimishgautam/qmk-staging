    if (is_keyboard_master()) {
        oled_render_layer_state();
        //oled_render_keylog();
    } else {
        oled_write("                     ", true);
        oled_write("  Nimish's Keyboard  ", true);
        oled_write("                     ", true);
    }
    return false;
}


